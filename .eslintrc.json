{
  "parserOptions": {
    "ecmaVersion": 8
  },
  "parser": "babel-eslint",
  "plugins": [
    "jest"
  ],
  "env": {
    "node": true,
    "es6": true,
    "mocha": true,
    "phantomjs": true,
    "jest/globals": true
  },
  "globals": {
    "expect": true
  },
  "extends": [
    "plugin:jest/recommended"
  ],
  "rules": {
    "no-reserved-keys": 0,
    "quote-props": 0,
    "no-extra-boolean-cast": 0,
    "guard-for-in": 0,
    "valid-jsdoc": 0,
    "no-console": 0,
    "no-debugger": 1,
    "no-constant-condition": 1,
    "no-empty": 1,
    "no-sparse-arrays": 1,
    "no-multi-str": 1,
    "comma-dangle": 2,
    "no-cond-assign": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-ex-assign": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "block-scoped-var": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": 2,
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undef-init": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "indent": [
      "error",
      2,
      {
        "SwitchCase": 1
      }
    ],
    "lines-around-comment": 2,
    "new-cap": 0,
    "new-parens": 1,
    "newline-after-var": 2,
    "no-array-constructor": 2,
    "no-inline-comments": 1,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "operator-assignment": 2,
    "operator-linebreak": 2,
    "quotes": [
      2, "single"
    ],
    "semi-spacing": 2,
    "semi": 2,
    "keyword-spacing": 0,
    "space-before-blocks": 2,
    "spaced-comment": 2,
    "wrap-regex": 2,
    "no-var": 0,
    "prefer-const": 0
  }
}